{
  "name": "node-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pm2 start ./bin/www --name node-server --watch ",
    "dev2": "pm2 start ecosystem.config.js --watch --env dev",
    "test": "pm2 start ecosystem.config.js --env test",
    "start": "pm2 start ecosystem.config.js --env production",
    "restart": "npm install && pm2 reload ecosystem.config.js --env production",
    "stop": "pm2 stop node-server"
  },
  "dependencies": {
    "async-validator": "^3.5.2",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "eslint-plugin-import": "^2.22.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "pg": "^8.6.0"
  },
  "devDependencies": {
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2"
  }
}
