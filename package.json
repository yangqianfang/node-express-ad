{
  "name": "node-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "pm2 start ecosystem.config.js --watch --env dev ",
    "test": "pm2 start ecosystem.config.js --env test",
    "start": "pm2 start ecosystem.config.js --env production",
    "restart": "npm install && pm2 reload ecosystem.config.js --env production",
    "stop": "pm2 stop node-server"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "pg": "^8.6.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "^2.3.2"
  }
}
